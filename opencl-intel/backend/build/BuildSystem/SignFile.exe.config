<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="Intel.CodeSigningService.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v2.0.50727" />
    <supportedRuntime version="v3.0" />
    <supportedRuntime version="v3.5" />
    <supportedRuntime version="v4.0" />
  </startup>
  <appSettings>
    <add key="CSSNameServers" value="http://ae-services.inn.intel.com/CertificateService/Service.asmx;http://scstraqs2new.sc.intel.com/CertificateService/Service.asmx" />
    <add key="CSSServers" value="http://ae-services.inn.intel.com/CertificateService/Service.asmx;http://scstraqs2new.sc.intel.com/CertificateService/Service.asmx" />
    <add key="VerboseMode" value="False" />
    <add key="DefaultCertificateName" value="DPDCertificate" />
    <add key="AdditionalCertificate" value="MSCV-EquifaxSecure.cer" />
    <add key="WorkingThreads" value="Auto" />
    <!-- Determines the number of workin threads. Auto - automatically detected number, otherwise any integer value-->
    <add key="LogMode" value="On" />
    <!-- On, Off -->
    <add key="LogFileName" value="Log.txt" />
    <add key="FileSigningMode" value="FilesOnly" />
    <!-- Determines signing method. Possible values: FilesOnly, CatalogOnly, FilesAndCatalog-->
    <add key="CertificateRequestAttempts" value="10" />
    <!-- The number of attempts to get certificate in case of network errors -->
    <add key="DelayBetweenRequestAttempts" value="10" />
    <!-- The delay between request attempts in seconds-->
    <add key="ErrorRecoveryAttempts" value="10" />
    <!-- The number of attempts to recover different errors -->
    <add key="DelayBetweenRecoveryAttempts" value="10" />
    <!-- The delay between attempts to recover different errors, in seconds-->
    <add key="AlwaysSignKernelDriversWithAdditionalCertificate" value="True" />
  </appSettings>
  <applicationSettings>
    <Intel.CodeSigningService.Properties.Settings>
      <setting name="SignFile_CertificateService_Signer" serializeAs="String">
        <value>http://localhost/CertificateService/Service.asmx</value>
      </setting>
    </Intel.CodeSigningService.Properties.Settings>
  </applicationSettings>
</configuration>