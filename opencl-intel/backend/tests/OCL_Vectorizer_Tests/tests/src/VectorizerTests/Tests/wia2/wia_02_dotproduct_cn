__kernel void wia_02_dotproduct_cn( __global const int *in,
		__global double2 *out)
{
	int gid = get_global_id(0);
	double4 x=(gid,gid+1,gid+2,gid+3), y=(gid,gid,gid,gid); //cons*cons
	int i;
	double z=0;
	
	
	for (i=0;i<12000;i++)
	{
		if( z>12)  //in
		{
			x=x+1; 
			y=y-2; 
			z=dot(x,y);   //cons*cons*
			i=i+2+z;
		}	
	
		out[gid]+=(z,dot(y,y+1));  //cn*cn
	}	
	
	
	
	
	
	
	
}