__kernel void unstructed_cf6( __global const float2 *in,
		__global float2 *out)
{
	int gid = get_global_id(0);
	
	long i;
	long sum=0;
	float fk=0;
	int flag=0;
	
	for (i=1; i<1000; i++,sum+=i) 
	{
		label1:
		fk=1;
		while (fk<100) 
		{
			if (i+fk>50) 
			{
				label2:
				i=i+1;
				fk=fk+sum+2+in[gid].x+in[gid].y;
				sum=sum+1;
				goto label7;
			}
			else
			{
				label3:
					i=i-1;
					fk=0;
					sum=sum*2;
					goto label7;
			}
			i=0;
			goto label1;
			i=1000;
			sum=-sum;
			label8:
			fk=fk+12;
			for (int m=1;m<100;m++)
			{
				i=i+2;
				out[gid].x=out[gid].x-1;
				out[gid].y=out[gid].y+1;
			}
		}
		label7:
		i=i+12;
		if( fk<324)
		{
			goto label8;
		}
		else if (fk>600)
		{
			goto label9;
		}
	}	
	for (i=1001;i>0;i-=126)
	{
		label9:
		out[gid]=12+out[gid];
		i=i-3;
		fk=fk+12;
		sum=sum-i-fk;
		if (flag<10)
		{
			flag=flag+1;
			goto label1;
		}	
		flag++;
		
	}
	
	out[gid]=out[gid]+gid+flag;
}	