__kernel void wia_45_fptoui ( __global  float *in,__global uint8 *out)
{
	int gid=get_global_id(0);	
	uint i=2,j,k,l;
	out[gid] = 0;    //UNIFORM
	
	uint i_value1_u=1,i_value2_u=in[1];
	uint2 i2_value1_u=1,i2_value2_u=in[1];
	
	uint i_value1_c=1+gid,i_value2_c=in[1]+gid;  //cons
	uint2 i2_value1_c=1+gid,i2_value2_c=in[1]+gid;
	
	int i_value1_s=(1+gid)*2,i_value2_s=(in[1]+gid)*4;  //str
	int2 i2_value1_s=(1+gid)*i+1,i2_value2_s=(in[1]+gid)*i; //str
	
	float f_value1_u=1.,f_value2_u=in[1];
	float2 f2_value1_u=1,f2_value2_u=in[1];
	
	float f_value1_c=1.+gid,f_value2_c=in[1]+gid;  //cons
	float2 f2_value1_c=1+gid,f2_value2_c=in[1]+gid;
	
	float f_value1_s=(1+gid)*2,f_value2_s=(in[1]+gid)*4;  //str
	float2 f2_value1_s=(1+gid)*i+1,f2_value2_s=(in[1]+gid)*i; //str
	
	double d_value1_u=1.,d_value2_u=in[1];
	double2 d2_value1_u=1,d2_value2_u=in[1];
	
	double d_value1_c=1.+gid,d_value2_c=in[1]+gid;  //cons
	double2 d2_value1_c=1+gid,d2_value2_c=in[1]+gid;
	
	double d_value1_s=(1+gid)*2,d_value2_s=(in[1]+gid)*4;  //str
	double2 d2_value1_s=(1+gid)*i+1,d2_value2_s=(in[1]+gid)*i; //str
	
	out[gid].s0 =f_value1_u;  //uniform
	for (i=1;i<256;i++) 
	{
		out[gid].s0+=f_value1_u;   //UNIFORM
		for (j=1;j<3;j++)
		{
			out[gid].s2 -=f_value2_u;
			for (j=1;j<14;j+=2) 
			{
				out[gid].s01 =f_value1_c+out[gid].s0;
			}
		}
		out[gid].s01 =f_value1_c+out[gid].s0+out[gid].s3;
	}
	
	out[gid].s0 =f_value1_c;

}