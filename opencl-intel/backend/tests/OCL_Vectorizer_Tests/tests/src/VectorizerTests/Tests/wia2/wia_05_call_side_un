
 void test1( int *rez,int i);
 int test2( int k,int gid);
 int test3( int k,int gid);

__kernel void wia_05_call_side_un( __global const int *in,
		__global float2 *out)
{
	int gid = get_global_id(0);
	double2 sum=(0,0);
	int i,j;
		for (i=1;i<1000;i++)
		{		
				test1(&i,gid+7);  //addres of i is rnd second paramter is cons
			
			    out[gid].y=sum.x+sum.y+gid+test3(in[gid]*2,gid);  //call with rnd para,
			
				sum.y=sum.y+test2(2,12); //call with uniform parameters			
		}
	
	out[gid].x=sum.x+sum.y;	
}

 void test1( int *rez,int i)  //for not uniform return rand
{
	 rez=i+i+1;	
	 return;
}

 int test2( int k, int gid)   //for not uniform return rand
{
	return gid+2+k;	
}

 int test3( int k,int gid)
{
	int i=0;	
	while (i>100)
	{
		i++;
		i+=test2(i,i);  //uniform
	}
}


