__kernel void unstructed_cf79( __global const float2 *in,
		__global float2 *out)
{
	int gid = get_global_id(0);
	
	long i;
	long sum=0;
	float fk=0;
	
	for (i=1; i<10; sum+=i) 
	{
		label1:
		fk=1;
		while (fk<100) 
		{
			if (i+fk>50) 
			{
				i=i+1;
				fk=fk+sum+2+in[gid].x+in[gid].y;
				goto label7;
			}
					i=i-1;
					fk=0;
					sum=sum*2;
					goto label7;
			goto label1;
			label8:
			fk=fk+12;
			for (int m=1;m<100;m++)
			{
				i=i+2;
				out[gid].x=out[gid].x-1;
				out[gid].y=out[gid].y+1;
			}
		}
		label7:
		i=i+12;
		if( fk<324)
		{
			goto label8;
		}
		if (fk>600)
		{
			goto label9;
		}
	}	
	for (i=1001;i>0;i-=126)
	{
		label9:
		out[gid]=1+out[gid];
	}
}	