// Tests icmp instruction via WIA.
// The The 'fcmp' instruction returns a boolean value or a vector of boolean values based on comparison of its two integer, 
// great  equal is tests
// integer vector, or pointer operands.
// un*un

__kernel void wia_19_fcmp_ge_uu(__global float *in,__global int4 *out,const ptrdiff_t intPtrArg1)
{
	
int gid = get_global_id(0);
	
int4 float4Arg1= (1,123,232,3); //un
	
int4 float4Arg2= (1,2,3,4);//un

int4 float4Tmp=float4Arg1+float4Arg2;
	
int i;

//need to enforce fcmp execution
for (i=1;i<1000;i++) 
{
	float4Arg2+=0.1;
	float4Arg1*=1.0001;
}

int4 flag =(float4Arg1<=float4Arg2);



	
	if (flag.s0>0)
	{
		for (i=1;i<32767;i+=8)   
		{
			//printf("\n gid=%d i=%d\n",gid,i);
			float4Tmp=(float4Arg1<=float4Arg2)*4;

			out[gid]+= float4Tmp+1;
			
		}
	}
	else
	{
		for (i=1099;i>2;i-=8)   
		{
			//printf("\n gid=%d i=%d\n",gid,i);
			float4Tmp= (float4Arg1<=float4Arg2)*4;
			
			out[gid]+= float4Tmp+1;
			
		}
		
		
		
	}
	

}