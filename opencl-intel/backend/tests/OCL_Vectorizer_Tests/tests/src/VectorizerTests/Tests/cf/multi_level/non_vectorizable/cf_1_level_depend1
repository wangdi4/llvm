__kernel void
cf_1_level_depend1 ( __global  double *in,
		__global float *out)
{
	int gid = get_global_id(0);
	long sum = 0;
	long sum_in = 0;
	

	for (int i = 0; i < 3; i++) {	
		sum=sum+1+i;		
		out[gid+10]= sum*sum_in+i;		
		for (int j=0; j<6;j=j+2) {
			out[gid]= sum*sum_in+i*j;
			//printf("\n@@@ gid=%d  out[gid+j]=%lf\n@@@",gid,out[gid+j]);
		}
		
	}
	out[gid]=out[gid] - (float) (sum+gid+sum_in);

}	
