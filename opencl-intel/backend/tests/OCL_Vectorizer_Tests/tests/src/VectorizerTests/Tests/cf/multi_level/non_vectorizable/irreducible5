__kernel void irreducible5 ( __global const long *in,
		__global float *out)
{
	int gid = get_global_id(0);
	int i=gid+2;
	int j=0;
	float sum=0;
	//printf("\n before first for  gid=%d\n",gid);
	
	for (i=1; i < 20*abs(in[gid+1]);i=abs(in[gid+1])+i+2)
	{
		sum=sum+i;
		//printf("\n i=%d",i);
	}	
		
	
	if (i>3.5*gid) {
		label0:
		//printf("\n after label0\n");
			j=j+1;
			goto label1;
	}
	else {
		label1:
		//printf("\n after label1\n");
			j=j*2;
			if (j>20) {
				goto label2;
			}
			goto label0;
	
	}
	label2:
	//printf("\n befor senond for line 31 \n");
	for (i=1; i < 10000;i=i+2)
	{
		sum=sum+i;
	}
	
	if (i+sum>20.5*gid) {
		label4:
			j=j+2;
			i=i+gid;
			goto label5;
	}
	else {
		label5:
			//printf("\n line 46 j=%d\n",j);
			j=j*2;
			sum=sum-12;
			if (j>240) {
				goto label6;
			}
			goto label4;
	
	}
	
	label6:
	
	
	out[gid] = i* gid + j;
	
	
	

}