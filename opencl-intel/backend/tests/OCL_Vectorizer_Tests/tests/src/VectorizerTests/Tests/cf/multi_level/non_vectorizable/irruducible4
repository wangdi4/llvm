__kernel void irruducible4 ( __global const long *in,
		__global float *out)
{
	int gid = get_global_id(0);
	int i=gid+2;
	int j=0;
	float sum=0;
	
	
	for (i=1; i < abs(20*in[gid+1]);i=abs(in[gid+1])+i+2)
	{
		sum=sum+i;
	}	
		
	
	if (i>3.5*gid) {
		label0:
			j=j+1;
			goto label1;
	}
	else {
		label1:
			j=j*2;
			if (j>20) {
				goto label2;
			}
			goto label0;
	
	}
	label2:
	//printf("\n hello \n");
	for (i=1; i < abs(in[gid]*20);i=i+(abs(in[gid]*3))+1)
	{
		sum=sum+i;
	}
	
	
	out[gid] = i* gid + j;
	
	
	

}