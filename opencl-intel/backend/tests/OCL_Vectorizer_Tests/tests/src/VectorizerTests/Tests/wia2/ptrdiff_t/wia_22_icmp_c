// Tests icmp instruction via WIA.
// The The 'icmp' instruction returns a boolean value or a vector of boolean values based on comparison of its two integer, 
// integer vector, or pointer operands.

__kernel void wia_22_icmp_c(__global int *in,__global float4 *out,const ptrdiff_t intPtrArg1)
{
	int gid = get_global_id(0);
	int4 int4Arg1=(int4) (1,2,3,4); //unif
	int4 int4Arg2=(int4) (gid,gid+1,gid+2,gid-1); //cons
	int4 int4Tmp=int4Arg1+int4Arg2;
	int i;
	
	for (i=1;i<32224;i+=8)   //unif
	{
		//printf("\n gid=%d i=%d\n",gid,i);
		int4Tmp=(int4Arg1==int4Arg2)*4;
		i=i+int4Tmp.s0+int4Tmp.s1-int4Tmp.s2;
		out[gid]+=int4Tmp+1+gid;
		
	}
	
	

}