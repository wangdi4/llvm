//rename me to cf7 

__kernel void unstructed_cf1( __global const float2 *in,
		__global float2 *out)
{
	int gid = get_global_id(0);
	
	long i;
	long sum=0;
	float fk=0;
	
	//printf("Tid = %d\n", gid);
	for (i=1; i<100; i+=40)  {
		fk=1;
		if (i>49) {
			//printf("tid99 = %d\n", gid);
			fk=1+in[gid].x+in[gid].y;				
		}	
		
			
		while (fk < 324) {		
			fk=fk+12;
			for (int m=1;m<30;m++) {
				i=i+2;
				out[gid].y+=1;
				//printf("tid = %d\n", gid);
			}			
		}
		
	}	
	
	for (i=1001;i>0;i-=126)	out[gid]=12+out[gid];

}	