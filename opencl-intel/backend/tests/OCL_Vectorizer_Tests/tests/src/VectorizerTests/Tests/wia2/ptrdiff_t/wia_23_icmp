// Tests icmp instruction via WIA.
// The The 'icmp' instruction returns a boolean value or a vector of boolean values based on comparison of its two integer, 
// integer vector, or pointer operands.








__kernel void wia_23_icmp(__global float2 *in,__global long4 *out,const ptrdiff_t intPtrArg1)
{
	int gid = get_global_id(0);
	double4 double4Arg1=(double4) (in[gid].s0,1,in[gid].s1,(2+in[gid]).s1); //rand
	double4 double4Arg2=(double4) ((in[gid]+2).s0,in[gid].s1,in[gid].s0,(2+in[gid]).s1); //rand
	
	out[gid]= (double4Arg1==double4Arg2);
	
	
	
	
	
	
	//tmp = tmp + tmp;
	//out[gid] = as_float8(tmp);

}