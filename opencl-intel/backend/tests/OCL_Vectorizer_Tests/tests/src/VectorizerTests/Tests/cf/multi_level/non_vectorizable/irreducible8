__kernel void irreducible8 ( __global const long3 *in,
		__global float *out)
{
	int gid = get_global_id(0);
	int i=gid+2*((int) in[gid].x);
	int j=0;
	float k=1;
	float sum=0;
	

	while (j>=0) {
		j=j+2;
		for (k=1; k<1000;k=k+1)
		{
			label1:
			sum=in[gid].x+in[gid].y;
			if ((long) sum%3 == 0)
			{
				i=i+1;
				j=j+100;
				goto label3;
			}
			else if ((long) sum%4 == 1)
			{
				i=i-1;
				j=j+100;
				goto label3;
			}
			else if ((long) sum%5 ==2)
			{
				i=i*2;
				j=-10;
				goto label6;	
			}
			label6:
			if (i>1000 || i<10) 
			{
				j=1;
			}
		}
		label3:
		k=k+100;
		j=j-2;
		sum=sum-((float)  in[gid].z);
	}
	
		
	
	
	out[gid] = i* gid + j- (int) sum;
	
	
	
	

}