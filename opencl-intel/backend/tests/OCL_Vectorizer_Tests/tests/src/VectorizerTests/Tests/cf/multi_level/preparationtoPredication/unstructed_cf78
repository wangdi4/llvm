__kernel void unstructed_cf78( __global const float2 *in,
		__global float2 *out)
{
	int gid = get_global_id(0);
	long i;
	long sum=0;
	float fk=0;
	for (i=1; i<10; sum+=i) 
	{
		label1:
		while (fk<100) 
		{
			if (i+fk>50) 
			{
				i=i+1;
				fk=fk+sum+in[gid].y;
			}
			else			
				fk=0;
			goto label7;
			label8:
			fk=fk+12;			
			out[gid].y=out[gid].y+1;
			i=i+200;
		}
		label7:
		i=i+12;
		if( fk<324)
		{
			goto label8;
		}
		if (fk>600)
		{
			goto label9;
		}
	}	
	for (i=1001;i>0;i-=126)
	{
		label9:
		out[gid]=1+out[gid];
	}
}	