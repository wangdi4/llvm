; ModuleID = 'Program'
; This module is used by opaque-type-handling.ll
target datalayout = "e-p:32:32:32-i1:8:8-i8:8:8-i16:16:16-i32:32:32-i64:64:64-f32:32:32-f64:64:64-f80:128:128-v64:64:64-v128:128:128-a0:0:64-f80:32:32-n8:16:32"
target triple = "i686-pc-win32"

%opencl.pipe_t = type opaque
%opencl.reserve_id_t = type opaque

define %opencl.reserve_id_t* @load(%opencl.reserve_id_t** %prid) {
entry:
  %ret = load %opencl.reserve_id_t** %prid, align 8
  ret %opencl.reserve_id_t* %ret
}

define %opencl.reserve_id_t* @callee(%opencl.pipe_t addrspace(1)* nocapture %p, %opencl.reserve_id_t** %prid, i32 %packet) nounwind {
entry:
;; %rid = call %opencl.reserve_id_t* @load(%opencl.reserve_id_t** %prid)
;; FIXME: Internal to the library function calls with opaque arguments are handled incorrectly.
;;        To overcome this issue __attribute__((always_inline)) may be used.
;;        The Module Verifier crashes with the following assertion:
;;            Call parameter type does not match function signature!
;;            %opencl.reserve_id_t** %prid
;;             %opencl.reserve_id_t.1**  %rid = call %opencl.reserve_id_t.1* @load(%opencl.reserve_id_t** %prid)
  %rid = load %opencl.reserve_id_t** %prid, align 8
  %conv = sext i32 %packet to i64
  %astype = inttoptr i64 %conv to %opencl.reserve_id_t*
  %null = bitcast i32* null to %opencl.reserve_id_t* 
  %cmp = icmp slt %opencl.reserve_id_t* %rid, %null
  br i1 %cmp, label %if.end, label %if.then

if.then:                                          ; preds = %entry
  %0 = bitcast %opencl.pipe_t addrspace(1)* %p to i32*
  store i32 %packet, i32* %0, align 4
  br label %if.end

if.end:                                           ; preds = %entry, %if.then
  ret %opencl.reserve_id_t* %astype
}
