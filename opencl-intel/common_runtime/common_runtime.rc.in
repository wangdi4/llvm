#include <windows.h>

#ifdef RC_INVOKED

VS_VERSION_INFO VERSIONINFO
FILEVERSION    @PRODUCTVER_MAJOR@,@PRODUCTVER_MINOR@,@BUILDVER_16_BIT_MAJOR@,@BUILDVER_16_BIT_MINOR@
PRODUCTVERSION @PRODUCTVER_MAJOR@,@PRODUCTVER_MINOR@,@BUILDVER_16_BIT_MAJOR@,@BUILDVER_16_BIT_MINOR@
FILETYPE       VFT_DLL

BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "FileDescription" ,"Intel(R) OpenCL(TM) Common Runtime Driver\0"
            VALUE "ProductName"     ,"Intel(R) OpenCL(TM) SDK"
            VALUE "LegalCopyright"  ,"@LEGALCOPYRIGHT@"
            VALUE "FileVersion"     ,"@PRODUCTVER_MAJOR@.@PRODUCTVER_MINOR@.@BUILDVER_16_BIT_MAJOR@.@BUILDVER_16_BIT_MINOR@"
            VALUE "ProductVersion"  ,"@PRODUCTVER_MAJOR@.@PRODUCTVER_MINOR@.@BUILDVER_16_BIT_MAJOR@.@BUILDVER_16_BIT_MINOR@"

            VALUE "CompanyName"     ,"@COMPANYNAME@"

            VALUE "InternalName"    ,"@TARGET_NAME@"
            VALUE "OriginalFilename","@TARGET_NAME@.dll"
        END
    END

    BLOCK "VarFileInfo"
    BEGIN
        // extend this line for localized versions
        VALUE "Translation", 0x0409, 0x04E4
    END
END

#endif
