/*
 * Part of the tests that check different LLVM instructions on different argument
 * types and different argument sources.
 *
 * Tests the LLVM vectorizable fmin function when it receives as input 2 double arguments
 * produced by previous NON vectorizable length function that receives arguments
 * produced by previous vectorizable round function.
 *
 * @param in
 * 		The input array.
 * @param out
 * 		The output array.
 *
 */
__kernel void
func_round_func_length_func_fmin ( __global const float *in,
		__global float8 *out)
{
	int gid = get_global_id(0);
	double arg1 = in[gid];
	double arg2 = in[gid] - 0.1931135;
	double arg3 = round((float)arg1);
	double arg4 = round((float)arg2);
	double arg5 = length((float)arg3);
	double arg6 = length((float)arg4);
	double arg7 = fmin((float)arg3, (float)arg4);
	out[gid].s0 = arg1;
	out[gid].s1 = arg2;
	out[gid].s2 = arg3;
	out[gid].s3 = arg4;
	out[gid].s4 = arg5;
	out[gid].s5 = arg6;
	out[gid].s6 = arg7;
}
