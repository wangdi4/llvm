/*
 * Part of the tests that check different LLVM instructions on different argument
 * types and different argument sources.
 *
 * Tests the LLVM fcmp instruction when it recieves as input double4 produced by
 * previous instruction and called twice.
 * This instruction is vectorizable.
 *
 * @param in
 *     The input array.
 * @param out
 *     The output array.
 *
 */
__kernel void
fcmp (  __global const float *in,
    __global float8 *out)
{
  int gid = get_global_id(0);
  double x = gid * tgamma(0.7541f);
  double y = atan2(in[gid], gid);
  double z = x - y + 600 / 3.322;
  double4 vec1 = (double4)(x, y * z, y -z, z);
  double4 vec2 = vec2;
  vec2.x = vec1.x + vec2.y;
  long4 res1 = vec1 == vec2;
  vec1.x = vec1.x - 1 + (gid / gid);
  long4 res2 = vec1 == vec2;
  long8 res;
  res.s0 = res1.x;
  res.s1 = res1.y;
  res.s2 = res1.z;
  res.s3 = res1.w;
  res.s4 = res2.x;
  res.s5 = res2.y;
  res.s6 = res2.z;
  res.s7 = res2.w;
  out[gid] = convert_float8(res);
}
