__kernel void infinite_loop5( __global const float *in,
		__global int2 *out)
{
	int gid = get_global_id(0);
	int i=0+gid/10000;
	int j=0;	
	int m=1;
	long3 sum=(0,0,0);
	//printf ("\n hello\n");
	
	if (i>-1) 
	{
		for (m=1;m<100;)
		{
			//printf("\n gid=%d\n",gid);

			if (i<200)
			{
				i=1+i;
				//printf("\n in the if >3 gid=%d\n",gid);
				out[gid].x=gid+i;
			}
			else if (i>400)
			{
				//printf("\n in the else string 25 gid=%d\n",gid);
				while (1==1)
				{
					//printf("\n in the while string 26  gid=%d\n",gid);
					if ( (j+gid)>12)
					{
						for (int k=1;k<1000;k++)
						{
							k=k+2;
							i=i+3;
						}
					}	
					else if	(gid<100)
					{
						for (i=1;i<100;i=i)
						{
							//printf("\n@@@@@@@@@@@@@@@@@@@@\n");
						}
					}
				}	
			}
			i=i+1;
			out[gid].x=out[gid].x+i+gid;
			if (gid==0 )
			{
				//printf("\n gid=%d\n",gid);
				m=101;
			}
			else
			{
				m+=1;
			}
				
			sum.x=i+gid;
		}
	}
	else
	{
		for (i=1;i<100;i++)
		{
			sum.x=i;
			sum.y=gid+sum.z;
		}
	}
	label1:
	out[gid].x = i* gid + j;
	out[gid].y = j* gid + i;	
}