
 int test1( int k,int gid);
 int test2( int k,int gid);

__kernel void amel9( __global const float *in,
		__global float2 *out)
{
	int gid = get_global_id(0);
	double2 sum=(0,0);
	int i,j;
		for (i=1;i<1000;i++)
		{
			if (gid==0)
			{
				sum.x=sum.x+=test1(j,gid);
			}
			else 
			{
				sum.y=sum.y+test2(j,gid);
			}
		}
	
	out[gid].x=sum.x+sum.y;
	
}


int test1( int k,int gid)
{
	return test2(k,gid);
	
}

int test2( int k, int gid)
{
	return 111;
	
}
