__kernel void unstructed_cf8( __global const float *in,
		__global int *out)
{
	int gid = get_global_id(0);
	int i=1;
	float j=0;
	double sum=0;
	
	label0:
	i=i+1;
	j=j +gid;
	sum=i+j;
	goto label7;
	
	label2:
	i=i/5;
	j=j+1;
	sum=1+sum-i;
	goto label6;
	
	label3: 
		if (sum>100 || i>100 || j>100) 
		{
			goto label8;
		}
		else 
		{
		 i=i+i+i;
		 j=j+12;
		 sum=sum*2;
		}
	goto label5;
	
	label4:
		i=i;
		j=j;
		sum=sum+i+j;
		for (int k=0;k<100;k++) 
		{	
			i=i+k;
		}	
		
		
	goto label0;	

	
	
	label5:
	if ( i+j> 200 || sum >1000) 
	{
		goto label8;
	}
	i=i+12;
	for (int k=0;k<300;k++) 
		{	
			j=i+k;
		}
	goto label4;
	
	
	
	label6:
	goto label3;
	
	
	
	label7:
	i=i-100;
	for (int k=0;k<100;k++) 
		{	
			sum=sum+gid;
			out[gid]=out[gid]+sum;
		}
	sum=0;
	goto label2;
	
	
	
	
	label8:
	
	
	out[gid] =out[gid]+  i* gid + j+ (int) sum;
	
	
	

}