__kernel void a_irreducible9 ( __global const long3 *in,
		__global float *out)
{
	int gid = get_global_id(0);
	int i=gid+2*((int) in[gid].x);
	int j=0;
	float k=1;
	float sum=0;
	int f=1;
	
	//printf("\n in[gid].x=%ld in[gid].y=%ld gid=%d \n",in[gid].x,in[gid].y,gid);
	

	while (j>=0) {
		j=j+2;
		for (k=1; k<1000;k=k+1)
		{
			label1:
			sum=in[gid].x+in[gid].y;
			if ((long) sum%3 == 0)
			{
				i=i+1;
				j=j+100;
				goto label7;
			}
			else if ((long) sum%4 == 1)
			{
				i=i-1;
				j=j+100;
				goto label3;
			}
			else if ((long) sum%5 ==2)
			{
				i=i*2;
				j=-10;
				goto label6;	
			}
			label6:
			if (i>1000 || i<10) 
			{
				j=1;
			}
		}
		label3:
		k=k+100;
		j=j-2;
		sum=sum-((float)  in[gid].z);
	}
	
	for (i=1;i<1000;i++)
	{
		label7:
			i=i-10;
			j=-20;
			k=k+20;
			sum=sum*1.5+in[gid].x;
		if (i+j+k+sum>65536 && f<5)
		{	
		    //printf("\n gid=%d f=%d\n",gid,f);
			f=f+1;
			goto label1;
		}
	}	
			
		
	
	
	out[gid] = i* gid + j- (int) sum;
	
	
	
	

}