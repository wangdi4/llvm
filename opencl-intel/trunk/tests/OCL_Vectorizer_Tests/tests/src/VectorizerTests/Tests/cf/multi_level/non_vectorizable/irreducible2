__kernel void irreducible2 ( __global const float *in,
		__global int2 *out)
{
	int gid = get_global_id(0);
	int i=gid+2;
	int j=0;	
	for (i=0;i<100+gid;i++)
	{
		//printf("\n in the for\n");
		j=j+1+gid+(int) in[gid];
		if (j>200+gid) 
		{
		goto label0;
		}
		else if (j<30)
		{
			goto label2;
		}
		j=j+i;				
	}	
	
	if (i>3.5*gid) {
		//printf("\n in the if i>3.5\n");
		label0:
			j=j+1;
			goto label1;
	}
	else {
		//printf("\n in the else  {\n");
		label1:
			j=j*2;
			if (j>20) {
				goto label2;
			}
			goto label0;	
	}
	label2:
	//printf("\n after label2 \n");
	out[gid].x = i* gid + j;
	out[gid].y = j* gid + i;	
}