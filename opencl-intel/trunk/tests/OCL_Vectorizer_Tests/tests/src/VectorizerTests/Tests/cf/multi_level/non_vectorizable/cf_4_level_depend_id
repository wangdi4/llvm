/*
 * Part of the control flow tests that check vectorization of control flow statements.
 *
 * Tests vectorization of 20 levels that don't depend on thread id - shouldn't be vectorized.
 *
 * @param in
 * 		The input array.
 * @param out
 * 		The output array.
 *
 */
__kernel void
cf_4_level_depend_id ( __global const float *in,
		__global float *out)
{
	int gid = get_global_id(0);
	long sum = 0;
	

	for (int i = 0; i < 10; i++) {
		for (int j = 0; j < 10; j++) {
			for (int k = 0; k < 10; k++) {
				for (int m = 0; m < 10; m++) {					
					sum+=i*j+k*m+gid;
				}
			}
		}	
	}
	out[gid]=sum+gid;
}	