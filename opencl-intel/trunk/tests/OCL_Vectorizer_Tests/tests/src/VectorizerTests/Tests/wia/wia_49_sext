__kernel void wia_49_sext ( __global  int *in,__global long4 *out)
{
	int gid=get_global_id(0);
	
	int x=in[1];		//
	int y=0;             //uniform
	int y_cons=gid;		//successive
	long y_un_small=0;   //uniform
	int i,j;
	float sum=0;        //uniform
	
	//calculate uniform y
	
	for (i=1;i<65537;i++)
	{
		y+=1;
		y+=i;
		y_un_small+=1;  //uniform+uniform
		if (i%4 == 0)
		{
			y=y+1+gid;    //stride
			
		}
	}	
	y_un_small+=gid+2;  //succesice
	
	
	for (i=1;i<2000;i++)
	{
		for (j=200;j>1;j-=2)
		{
			out[gid].s0 =y;
			out[gid].s1 =y_un_small;
			
			y=y+1;
			y_un_small+=2;
			
			sum+=out[gid].s0 + out[gid].s1;
		
		}
	
	}
	
	
	out[gid].s0 =y;
	out[gid].s1 =y_un_small;
	out[gid].s3=0;
	out[gid].s3+=gid; //succesive
	
	//printf("\n gid=%d y=%ld out[gid]=%d\n",gid,y,out[gid].s0);
	//printf("\n gid=%d y_un_small=%ld out[gid]=%d\n",gid,y_un_small,out[gid].s1);
	

}