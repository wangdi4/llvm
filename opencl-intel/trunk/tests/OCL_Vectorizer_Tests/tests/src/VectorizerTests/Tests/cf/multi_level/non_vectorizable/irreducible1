__kernel void irreducible1 ( __global const float *in,
		__global float *out)
{
	int gid = get_global_id(0);
	int i=gid+2;
	int j=0;
	
	if (i>3.5*gid) {
		label0:
			j=j+1;
			goto label1;
	}
	else {
		label1:
			j=j*2;
			if (j>20) {
				goto label2;
			}
			goto label0;
	
	}
	label2:
	out[gid] = i* gid + j;
	
	
	

}
