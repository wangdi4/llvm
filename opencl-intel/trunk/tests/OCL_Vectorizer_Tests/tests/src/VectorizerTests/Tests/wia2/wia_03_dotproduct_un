__kernel void wia_03_dotproduct_un( __global const int *in,
		__global double2 *out)
{
	int gid = get_global_id(0);
	double4 x=(1.,2.,3.,4), y=(1,3,5,7);
	int i;
	double z=0;
	
	
	for (i=0;i<2000;i++)
	{
		if( z>12)  //in
		{
			x=x+1; //un
			y=y-2; //un
			z=dot(x,y);   //unif*unif
			i=i+2+z;
		}	
	
		out[gid]+=(z,dot(y,y+1));  //un*un
	}	
	
	
	
	
	
	
	
}