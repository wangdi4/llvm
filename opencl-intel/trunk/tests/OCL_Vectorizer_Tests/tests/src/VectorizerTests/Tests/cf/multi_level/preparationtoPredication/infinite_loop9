__kernel void infinite_loop9 ( __global const float *in,
		__global int2 *out)
{
	int gid = get_global_id(0);
	int i=0+gid/10000;
	int j=0;	
	
	if (i>10) 
	{
		for (;;)
		{	
		printf("n gid\n");	
		}
	}
	else
	{
		for (i=1;i<100;i++)
		{
			;
		}
	}
	out[gid].x = i* gid + j;
	out[gid].y = j* gid + i;	
}