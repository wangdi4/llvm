__kernel void wia_52_fptrunc_to_r ( __global  double *in,__global float2 *out)
{
	int gid=get_global_id(0);
	
	double x=in[1];
	double y=0;             //uniform
	double y_cons=gid;
	double y_un_small=0;
	float i,j;
	float sum=0;
	
	//calculate uniform y
	
	for (i=1;i<65537;i+=1)
	{
		y+=1;
		y+=i;
		y_un_small+=1;
		if (i >200 || i < 20)
		{
			y=y+1+i*2;
			y_un_small+=2;
		}
	}	  //y is uniform and greater then max int  y_un_small is uniform and < max int
	
	
	
	for (i=1;i<2000;i+=2)
	{
		for (j=200;j>1;j-=2)
		{
			out[gid].s0 +=y;
			out[gid].s1 -=y_un_small;
			
			y=y+1;
			y_un_small+=2;
			
			sum+=out[gid].s0 + out[gid].s1;
		
		}
	
	}
	
	
	out[gid].s0 +=y;
	out[gid].s1 -=y_un_small;
	
	
	

}