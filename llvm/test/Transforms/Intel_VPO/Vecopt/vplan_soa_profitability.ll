  %arrayidx.current = phi ptr [ %arr_e.priv, %simd.begin.region], [%arrayidx.next, %simd.loop.end]
  %arrayidx.current1 = phi ptr [ %arr_ne.priv, %simd.begin.region], [%arrayidx.next1, %simd.loop.end]
