  %arrayidx.next1 = getelementptr inbounds i32, ptr %arrayidx.current1, i64 1
  %iv1.next = add nuw nsw i64 %iv1, 1
  %cmp = icmp ult i64 %iv1.next, 1024
  br i1 %cmp, label %simd.end.region, label %simd.loop

simd.end.region:
