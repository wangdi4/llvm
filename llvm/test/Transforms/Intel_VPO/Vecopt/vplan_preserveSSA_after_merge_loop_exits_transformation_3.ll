; CHECK-EMPTY:
; CHECK-NEXT:      [[CASCADED_IF_BLOCK1]]: # preds: [[CASCADED_IF_BLOCK0]]
; CHECK-NEXT:       i1 [[VP1:%.*]] = icmp eq i32 [[VP_EXIT_ID_PHI]] i32 2
; CHECK-NEXT:       br i1 [[VP1]], [[BB7:BB[0-9]+]], [[BB8:BB[0-9]+]]
; CHECK-EMPTY:
; CHECK-NEXT:        [[BB8]]: # preds: [[CASCADED_IF_BLOCK1]]
; CHECK-NEXT:         br [[BB9:BB[0-9]+]]
; CHECK-EMPTY:
; CHECK-NEXT:        [[BB7]]: # preds: [[CASCADED_IF_BLOCK1]]
; CHECK-NEXT:         br [[BB9]]
; CHECK-EMPTY:
; CHECK-NEXT:      [[BB6]]: # preds: [[CASCADED_IF_BLOCK0]]
; CHECK-NEXT:       br [[BB9]]
