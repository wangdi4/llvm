  %k1.iv.next = phi i32* [%k1.iv.n1, %then ], [%k1.iv.n2, %else ]
  %exitcond = icmp eq i64 %indvars.iv.next, %N
  br i1 %exitcond, label %for.cond.cleanup.loopexit, label %for.body

