  br label %inner.latch

inner.latch:
  %inner.iv.next = add nsw i32 %inner.iv, 1
  %inner.bottom.test = icmp eq i32 %inner.iv.next, %inner.tc
  br i1 %inner.bottom.test, label %bb1, label %inner.header

bb1:
