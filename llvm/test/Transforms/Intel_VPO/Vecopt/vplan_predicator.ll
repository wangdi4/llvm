;      Header <-----+
; (False) |         |
;  +----BB0 (D)     |
;  |    / (True)    |
;  |  BB1 (D)       |
;  | /   \          |
;  |/     \         |
;  BB2   BB3        |
;   \    /          |
;     BB4           |
;     |             |
;    Latch----------+
for.body:
  %indvars.iv = phi i32 [ 0, %entry ], [ %indvars.iv.next, %latch ]
