; CHECK-NEXT:     store i32 [[VP12]] i32* [[VP14]]
;
entry:
  br label %for.body

for.body:                                         ; preds = %for.body, %entry
  %i1.017 = phi i64 [ 0, %entry ], [ %inc, %for.body ]
  %add = add nsw i64 %i1.017, %n2
