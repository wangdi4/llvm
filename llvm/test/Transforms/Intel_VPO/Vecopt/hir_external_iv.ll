  %iv.inner = phi i64 [ 0, %DIR.OMP.SIMD.2 ], [ %iv.inner.inc, %omp.inner.for.body ]
  %arrayidx4 = getelementptr inbounds [100 x [100 x i64]], [100 x [100 x i64]]* @arr, i64 0, i64 %iv.outer, i64 %iv.inner
  %mul3 = mul nsw i64 %iv.outer, %iv.outer
