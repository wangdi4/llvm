; NOTE: Assertions have been autogenerated by utils/intel_update_vplan_checks.py
; Tests if a phi node is emitted for values that are defined in the loop body
; and they are used in the loop header (VP_DEF and VP_INNER_LOOP_INDUCTION) or
; outside of the loop body (VP_LIVE_OUT).

; RUN: opt < %s -vplan-func-vec -disable-output -print-after-vplan-func-vec-loop-exit-canon | FileCheck %s
; RUN: opt < %s -passes="vplan-func-vec" -disable-output -print-after-vplan-func-vec-loop-exit-canon | FileCheck %s

target datalayout = "e-m:e-i64:64-f80:128-n8:16:32:64-S128"
target triple = "x86_64-unknown-linux-gnu"

; Function Attrs: norecurse nounwind uwtable
define void @main() {
; CHECK-LABEL:  VPlan IR for: main
; CHECK-NEXT:    [[BB0:BB[0-9]+]]: # preds:
; CHECK-NEXT:     i32 [[VP_LANE:%.*]] = induction-init{add} i32 0 i32 1
; CHECK-NEXT:     br [[BB1:BB[0-9]+]]
; CHECK-EMPTY:
; CHECK-NEXT:    [[BB1]]: # preds: [[BB0]], [[NEW_LOOP_LATCH0:new.loop.latch[0-9]+]]
; CHECK-NEXT:     i32 [[VP_IV:%.*]] = phi  [ i32 0, [[BB0]] ],  [ i32 [[VP_IV_NEXT_SSA_PHI:%.*]], [[NEW_LOOP_LATCH0]] ]
; CHECK-NEXT:     i32 [[VP_DEF_PHI:%.*]] = phi  [ i32 0, [[BB0]] ],  [ i32 [[VP_DEF_SSA_PHI:%.*]], [[NEW_LOOP_LATCH0]] ]
; CHECK-NEXT:     i32 [[VP_VAR1:%.*]] = add i32 [[VP_IV]] i32 [[VP_DEF_PHI]]
; CHECK-NEXT:     i1 [[VP_CMP1:%.*]] = icmp eq i32 [[VP_VAR1]] i32 16
; CHECK-NEXT:     br i1 [[VP_CMP1]], [[BB2:BB[0-9]+]], [[INTERMEDIATE_BB0:intermediate.bb[0-9]+]]
; CHECK-EMPTY:
; CHECK-NEXT:      [[INTERMEDIATE_BB0]]: # preds: [[BB1]]
; CHECK-NEXT:       br [[NEW_LOOP_LATCH0]]
; CHECK-EMPTY:
; CHECK-NEXT:      [[BB2]]: # preds: [[BB1]]
; CHECK-NEXT:       i32 [[VP_DEF:%.*]] = add i32 [[VP_IV]] i32 2
; CHECK-NEXT:       br [[BB3:BB[0-9]+]]
; CHECK-EMPTY:
; CHECK-NEXT:      [[BB3]]: # preds: [[BB2]]
; CHECK-NEXT:       i32 [[VP_VAR2:%.*]] = add i32 [[VP_DEF]] i32 1
; CHECK-NEXT:       i1 [[VP_CMP2:%.*]] = icmp eq i32 [[VP_VAR2]] i32 32
; CHECK-NEXT:       br i1 [[VP_CMP2]], [[BB4:BB[0-9]+]], [[INTERMEDIATE_BB1:intermediate.bb[0-9]+]]
; CHECK-EMPTY:
; CHECK-NEXT:        [[INTERMEDIATE_BB1]]: # preds: [[BB3]]
; CHECK-NEXT:         i32 [[VP_LIVE_OUT:%.*]] = phi  [ i32 [[VP_VAR2]], [[BB3]] ]
; CHECK-NEXT:         br [[NEW_LOOP_LATCH0]]
; CHECK-EMPTY:
; CHECK-NEXT:      [[BB4]]: # preds: [[BB3]]
; CHECK-NEXT:       i32 [[VP_IV_NEXT:%.*]] = add i32 [[VP_IV]] i32 1
; CHECK-NEXT:       i1 [[VP_BOTTOM_TEST:%.*]] = icmp eq i32 [[VP_IV_NEXT]] i32 128
; CHECK-NEXT:       br [[NEW_LOOP_LATCH0]]
; CHECK-EMPTY:
; CHECK-NEXT:    [[NEW_LOOP_LATCH0]]: # preds: [[BB4]], [[INTERMEDIATE_BB0]], [[INTERMEDIATE_BB1]]
; CHECK-NEXT:     i32 [[VP_LIVE_OUT_SSA_PHI:%.*]] = phi  [ i32 undef, [[BB4]] ],  [ i32 undef, [[INTERMEDIATE_BB0]] ],  [ i32 [[VP_LIVE_OUT]], [[INTERMEDIATE_BB1]] ]
; CHECK-NEXT:     i32 [[VP_IV_NEXT_SSA_PHI]] = phi  [ i32 [[VP_IV_NEXT]], [[BB4]] ],  [ i32 undef, [[INTERMEDIATE_BB0]] ],  [ i32 undef, [[INTERMEDIATE_BB1]] ]
; CHECK-NEXT:     i32 [[VP_DEF_SSA_PHI]] = phi  [ i32 [[VP_DEF]], [[BB4]] ],  [ i32 undef, [[INTERMEDIATE_BB0]] ],  [ i32 [[VP_DEF]], [[INTERMEDIATE_BB1]] ]
; CHECK-NEXT:     i32 [[VP_EXIT_ID_PHI:%.*]] = phi  [ i32 0, [[BB4]] ],  [ i32 1, [[INTERMEDIATE_BB0]] ],  [ i32 2, [[INTERMEDIATE_BB1]] ]
; CHECK-NEXT:     i1 [[VP_TAKE_BACKEDGE_COND:%.*]] = phi  [ i1 [[VP_BOTTOM_TEST]], [[BB4]] ],  [ i1 false, [[INTERMEDIATE_BB0]] ],  [ i1 false, [[INTERMEDIATE_BB1]] ]
; CHECK-NEXT:     br i1 [[VP_TAKE_BACKEDGE_COND]], [[BB1]], [[CASCADED_IF_BLOCK0:cascaded.if.block[0-9]+]]
; CHECK-EMPTY:
; CHECK-NEXT:    [[CASCADED_IF_BLOCK0]]: # preds: [[NEW_LOOP_LATCH0]]
; CHECK-NEXT:     i1 [[VP0:%.*]] = icmp eq i32 [[VP_EXIT_ID_PHI]] i32 1
; CHECK-NEXT:     br i1 [[VP0]], [[BB5:BB[0-9]+]], [[CASCADED_IF_BLOCK1:cascaded.if.block[0-9]+]]
; CHECK-EMPTY:
; CHECK-NEXT:      [[CASCADED_IF_BLOCK1]]: # preds: [[CASCADED_IF_BLOCK0]]
; CHECK-NEXT:       i1 [[VP1:%.*]] = icmp eq i32 [[VP_EXIT_ID_PHI]] i32 2
; CHECK-NEXT:       br i1 [[VP1]], [[BB6:BB[0-9]+]], [[BB7:BB[0-9]+]]
; CHECK-EMPTY:
; CHECK-NEXT:        [[BB7]]: # preds: [[CASCADED_IF_BLOCK1]]
; CHECK-NEXT:         br [[BB8:BB[0-9]+]]
; CHECK-EMPTY:
; CHECK-NEXT:        [[BB6]]: # preds: [[CASCADED_IF_BLOCK1]]
; CHECK-NEXT:         i32 [[VP_VAR3:%.*]] = add i32 [[VP_LIVE_OUT_SSA_PHI]] i32 2
; CHECK-NEXT:         br [[BB8]]
; CHECK-EMPTY:
; CHECK-NEXT:      [[BB5]]: # preds: [[CASCADED_IF_BLOCK0]]
; CHECK-NEXT:       br [[BB8]]
; CHECK-EMPTY:
; CHECK-NEXT:    [[BB8]]: # preds: [[BB5]], [[BB6]], [[BB7]]
; CHECK-NEXT:     ret
; CHECK-NEXT:     br <External Block>
;
entry:
  %lane = call i32 @llvm.vplan.laneid()
  br label %inner_loop_header
;-------------------------------------------------------------------------------
;             BEFORE                                       AFTER
;-------------------------------------------------------------------------------
; +------>inner_loop_header-------+      +----------->BB1(inner_loop_header)
; |         /                     |      |          /                    |
; |       bb1                     |      |       BB2(bb1)                |
; |        |                      |      |          |                    |
; |       bb2 -----------+        |      |      BB3(bb2)       INTERMEDIATE_BB0
; |        |             |        |      |        /   \                  |
; +-inner_loop_latch  exitbb2  exitbb1   |       /     \                 |
;          |                             |    BB4     INTERMEDIATE_BB1   |
;       exitbb3                     (inner_loop_latch)       |           |
;                                        |          \        |           |
;                                        +----------NEW_LOOP_LATCH0<-----+
;                                                          |
;                                                  CASCADED_IF_BLOCK0
;                                                    /           \
;                                         (exitbb1)BB5   CASCADED_IF_BLOCK1
;                                                            /      \
;                                                 (exitbb2)BB6  (exitbb3)BB7
inner_loop_header:
  %iv = phi i32 [ 0, %entry ], [ %iv_next, %inner_loop_latch ]
  %def_phi = phi i32 [ 0, %entry ], [ %def, %inner_loop_latch ]
  %var1 = add nsw i32 %iv, %def_phi
  %cmp1 = icmp eq i32 %var1, 16
  br i1 %cmp1, label %bb1, label %exitbb1

bb1:
  %def = add nsw i32 %iv, 2
  br label %bb2

bb2:
  %var2 = add nsw i32 %def, 1
  %cmp2 = icmp eq i32 %var2, 32
  br i1 %cmp2, label %inner_loop_latch, label %exitbb2

inner_loop_latch:
  %iv_next = add nsw i32 %iv, 1
  %bottom_test = icmp eq i32 %iv_next, 128
  br i1 %bottom_test, label %inner_loop_header, label %exitbb3

exitbb1:
  br label %end

exitbb2:
  %live_out = phi i32 [ %var2, %bb2 ]
  %var3 = add nsw i32 %live_out, 2
  br label %end

exitbb3:
  br label %end

end:
  ret void
}

declare i32 @llvm.vplan.laneid()
